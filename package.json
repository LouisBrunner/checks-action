{
  "name": "checks-action",
  "version": "2.0.0",
  "private": true,
  "description": "GitHub Action which wraps calls to GitHub Checks API",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint '**/*.js' '**/*.ts'",
    "pack": "ncc build -m",
    "test": "jest",
    "format": "prettier --write '**/*.js' '**/*.ts'",
    "all": "npm run build && npm run lint && npm run pack && npm test"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/LouisBrunner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisBrunner/checks-action.git"
  },
  "keywords": [
    "github",
    "actions",
    "checks"
  ],
  "author": "Louis Brunner",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/rest": "^21.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.18.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}

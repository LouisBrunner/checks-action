{
  "name": "checks-action",
  "version": "1.6.1",
  "private": true,
  "description": "GitHub Action which wraps calls to GitHub Checks API",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint '**/*.js' '**/*.ts'",
    "pack": "ncc build -m",
    "test": "jest",
    "format": "prettier --write '**/*.js' '**/*.ts'",
    "all": "npm run build && npm run lint && npm run pack && npm test"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/LouisBrunner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LouisBrunner/checks-action.git"
  },
  "keywords": [
    "github",
    "actions",
    "checks"
  ],
  "author": "Louis Brunner",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^20.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
